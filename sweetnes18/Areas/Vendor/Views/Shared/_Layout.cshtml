                <nav aria-label="Page navigation example">
                    <ul class="pagination">
                        <li class="page-item">
                            <a class="page-link" aria-label="Previous" ng-click="decrement();pagex();">
                                <span aria-hidden="true">&laquo;</span>
                                <span class="sr-only">Previous</span>
                            </a>
                        </li>
                        <li class="page-item" ng-repeat="n in [] | range:numberOfPages()">
                            <a class="page-link" ng-click="assign(n);pagex();">{{n}}</a>
                        </li>
                        <li class="page-item">
                            <a class="page-link" aria-label="Next" ng-click="increment();pagex();">
                                <span aria-hidden="true">&raquo;</span>
                                <span class="sr-only">Next</span>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
    </script>
